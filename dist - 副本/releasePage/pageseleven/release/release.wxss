page{background-color:#fff}textarea{width:100%}.up-img{display:flex;flex-wrap:wrap}.up-img .up-btn,.up-img image{width:210rpx;height:210rpx;margin:10rpx}.up-img .up-btn{display:inline-block;border:2rpx dashed #bbb;position:relative}.up-img .up-btn:before{font-size:100rpx;position:absolute;color:#bbb}.up-img .up-btn:before,.up-img .up-progress{top:50%;left:50%;transform:translate(-50%,-50%)}.up-img .up-progress{width:210rpx;height:210rpx}.up-img .up-progress .tac{padding:2rpx 6rpx;border-radius:20rpx;background-color:#eee}.bottom-wrap{display:flex;align-items:center}.bottom-wrap .count{flex:1;word-break:keep-all}.tips{display:flex;align-items:center;flex-wrap:wrap}.tips .tips-item{margin:10rpx;margin-left:0;border:2rpx solid #999;padding:4rpx 10rpx;transition:all .5s}.tips .active{background-color:#ff3b5b;color:#fff;border-color:#ff3b5b}.release-btn{position:fixed;bottom:30rpx;left:50%;width:80%;text-align:center;height:88rpx;line-height:88rpx;background-color:#ff3b5b;transform:translateX(-50%);border-radius:80rpx;box-shadow:0rpx 6rpx 10rpx 4rpx rgba(238,200,211,.7)}