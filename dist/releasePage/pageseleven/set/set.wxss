.shop-img,.user-name{display:flex;align-items:center;margin-top:1rpx}.shop-img{justify-content:space-between}.shop-img image{width:48rpx;height:48rpx}.save{background-color:#fff;line-height:80rpx;font-size:28rpx}.up-img{display:flex;flex-wrap:wrap;justify-content:flex-start}.up-img .up-btn,.up-img image{width:200rpx;height:200rpx;margin:10rpx}.up-img .up-btn{display:inline-block;border:2rpx dashed #bbb;position:relative}.up-img .up-btn:before{font-size:100rpx;position:absolute;color:#bbb}.up-img .up-btn:before,.up-img .up-progress{top:50%;left:50%;transform:translate(-50%,-50%)}.up-img .up-progress{width:200rpx;height:200rpx}.up-img .up-progress .tac{padding:2rpx 6rpx;border-radius:20rpx;background-color:#eee}