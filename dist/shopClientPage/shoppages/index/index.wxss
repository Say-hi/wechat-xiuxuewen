page{background-color:#f1f1f1}.search{padding:20rpx 30rpx;position:relative;border-radius:8rpx;margin-top:2rpx}.search,.search .s-wrap{display:flex;align-items:center;background-color:#fff}.search .s-wrap{border-radius:65rpx;border:2rpx solid #999;flex:1;padding-left:20rpx}.search input{flex:1;padding-left:20rpx;height:65rpx;line-height:65rpx;background-color:#fff;margin-right:20rpx}.search .cancel{margin-left:30rpx}.client-item{background-color:#fff4f6}.client-item view{display:flex;align-items:center;justify-content:space-between;margin-top:20rpx}.client-item view:first-child{margin-top:0;justify-content:flex-start}.client-item image{width:80rpx;height:80rpx}