.mask,
.mask-t {
  position: fixed;
  z-index: 9999;
  bottom: 0;
  left: 0;
  background-color: rgba(0, 0, 0, 0.5);
  width: 100%;
  height: 100%;
}
.mask button,
.mask-t button {
  padding: 0;
  margin: 0;
  line-height: 50rpx;
  font-size: 30rpx;
  background-color: transparent;
  border: none;
}
.mask button:after,
.mask-t button:after {
  border: none;
  background-color: transparent;
}
.mask .mask-in,
.mask-t .mask-in {
  position: absolute;
  min-width: 600rpx;
  top: 50%;
  left: 50%;
  background-color: #fff;
  transform: translate(-50%, -50%);
}
.mask .mask-in text,
.mask-t .mask-in text {
  color: #ff6501;
}
.user-mask {
  background: #ccc !important;
  z-index: 10000;
}
.mask-btn {
  display: flex;
  color: #16a1fe;
  text-align: center;
  line-height: 112rpx;
  border-top: 1rpx solid #dcdcdc;
}
.mask-btn view {
  flex: 1;
}
.mask-btn .cancel {
  border-right: 1rpx solid #dcdcdc;
}
.mask-t {
  background-color: rgba(0, 0, 0, 0.1);
}
.mask-t .mask-toast {
  position: absolute;
  top: 50%;
  left: 50%;
  background-color: #fff;
  border-radius: 10rpx;
  padding: 10rpx;
  transform: translate(-50%, -50%);
  text-align: center;
}
.mask-t .mask-toast .mask-title {
  line-height: 60rpx;
  font-size: 30rpx;
  color: #333;
}
.mask-t .mask-toast .mask-image {
  width: 80rpx;
  height: 80rpx;
}
.mask-t .mask-toast .gzh-img {
  width: 500rpx;
  height: 275rpx;
}
.mask-t .mask-toast .mask-content {
  line-height: 60rpx;
  font-size: 28rpx;
  color: #333;
}
.mask-t .mask-toast .add-qun {
  text-align: left;
  font-size: 26rpx;
  line-height: 40rpx;
}
.mask-t .mask-toast .btn {
  text-align: center;
  color: #09ba06;
  width: 100% !important;
  font-size: 36rpx;
  line-height: 100rpx;
  border-top: 1rpx solid #b7bfc0;
  margin: 0 !important;
}
.mask-t .gzh-w {
  padding: 0;
  padding-top: 20rpx;
  min-width: 500rpx;
}
.mask-t .gzh-w image {
  margin: 34rpx;
}
.mask-t .gzh-w .mask-content {
  padding: 0 20rpx;
}
.mask-t .code-add input {
  border: 1rpx solid #333;
  margin: 20rpx 0;
  width: 500rpx;
  padding: 10rpx;
}
.mask-t .code-add .mask-btn-wrap {
  display: flex;
  border-top: 1rpx solid #eee;
  align-items: center;
  line-height: 80rpx;
}
.mask-t .code-add .mask-btn-wrap .mask-btn {
  flex: 1;
  font-weight: 700;
  border-right: 1rpx solid #eee;
}
.mask-t .code-add .mask-btn-wrap .mask-btn:nth-child(2) {
  border-right: none;
  color: #09bb07;
}
.gzh {
  background-color: rgba(0, 0, 0, 0.5);
}
.mask-input {
  background-color: rgba(0, 0, 0, 0.5);
}
.mask-input .mask-toast {
  min-width: 80%;
  padding-bottom: 90rpx;
}
.mask-input .mask-toast input {
  margin: 20rpx 0;
  border: 1rpx solid #eee;
  border-radius: 10rpx;
  font-size: 30rpx;
}
.mask-input .mask-toast .mask-btn {
  position: fixed;
  left: 0;
  bottom: 0;
  width: 100%;
  display: flex;
  align-items: center;
  line-height: 80rpx;
}
.mask-input .mask-toast .mask-btn view {
  flex: 1;
  text-align: center;
}
.mask-input .mask-toast .mask-btn .cancel {
  background-color: #eee;
  border-bottom-left-radius: 10rpx;
}
.mask-input .mask-toast .mask-btn .confirm {
  color: #fff;
  background-color: #22ac38;
  border-bottom-right-radius: 10rpx;
}
.settop {
  float: right;
  padding: 10rpx;
  padding-top: 0;
}
.message-count {
  position: fixed;
  bottom: 20rpx;
  left: 62.5%;
  transform: translateX(-50%);
  padding: 10rpx 15rpx;
  background-color: #22ac38;
  color: #fff;
}
.message-count::after {
  content: '';
  width: 20rpx;
  height: 20rpx;
  background-color: #22ac38;
  position: absolute;
  bottom: 0;
  left: 50%;
  transform: translate(-50%, 50%) rotateZ(45deg);
}
.small {
  height: 0;
  transition: all 0.5s;
}
.open-setting {
  position: absolute;
  top: 50%;
  left: 50%;
  z-index: 99999;
  transform: translate(-50%, -50%);
  width: 600rpx;
  height: 400rpx;
  text-align: center !important;
  display: flex !important;
  align-items: center !important;
}
.open-setting button {
  width: 100% !important;
  background: #fff !important;
  line-height: 80rpx !important;
  text-align: center !important;
}
.bottom-operation {
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  background-color: #fff;
}
.goback {
  border-radius: 50%;
  position: fixed;
  bottom: 140rpx;
  width: 100rpx;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  right: 30rpx;
  border: 1rpx solid #eee;
  transition: all 0.5s;
}
.move {
  animation: 0.5s movein linear forwards;
}
.moveother {
  width: 0;
  height: 0;
}
@keyframes movein {
  from {
    bottom: 0rpx;
    transform: scale(0.1);
  }
  80% {
    transform: scale(1.2);
  }
  to {
    bottom: 140rpx;
    transform: scale(1);
  }
}
