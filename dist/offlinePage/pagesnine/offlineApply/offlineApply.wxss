page {
  height: 100%;
}
.tab-top {
  display: flex;
  align-items: center;
  justify-content: space-around;
  height: 7%;
}
.tab-top .t-t-item {
  position: relative;
}
.tab-top .t-t-item:after {
  content: '>';
  color: #9a9a9a;
  font-size: 40rpx;
  position: absolute;
  top: 50%;
  right: -60rpx;
  transform: translate(0, -50%);
}
.tab-top .t-t-item:nth-last-child(1):after {
  content: '';
}
swiper {
  height: 93%;
}
.goods {
  height: 100%;
}
.address {
  background-color: #fff;
  position: relative;
  margin-bottom: 20rpx;
}
.address image {
  width: 100%;
}
.address .address-s {
  position: absolute;
  top: 0;
  left: 0;
  width: 90%;
  padding: 0 20rpx;
  background-color: #ffffff;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  height: 120rpx;
}
.address .address-s .a-s-t {
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.address .open-setting {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, 0.5);
  text-align: center;
  display: flex;
  align-items: center;
  transform: translate(0, 0);
}
.address .open-setting button {
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  width: 80% !important;
}
.goods-info {
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.choose-gender {
  display: flex;
  align-items: center;
  justify-content: space-around;
}
.choose-gender image {
  width: 124rpx;
  height: 124rpx;
  display: inline-block;
}
.choose-gender .gender-circle {
  width: 22rpx;
  height: 22rpx;
  border: 4rpx solid #979797;
}
.choose-gender .gender-active {
  border-color: #FF3656;
}
.choose-gender .c-g-item {
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-top: 160rpx;
}
.user-input {
  display: flex;
  border: 2rpx solid #666;
  border-radius: 50rpx;
  align-items: center;
  padding: 0 30rpx;
  line-height: 88rpx;
  height: 88rpx;
  margin: 0 30rpx;
  margin-top: 30rpx;
}
.user-input input {
  flex: 1;
  line-height: inherit;
  padding-left: 20rpx;
}
.name-gender {
  display: flex;
  justify-content: space-between;
}
.user-input-2 {
  display: flex;
  flex-direction: column;
}
.user-input-2 .u-i-2-t {
  padding-left: 30rpx;
}
.user-input-2 input {
  box-sizing: border-box;
  border-radius: 88rpx;
  padding: 0 30rpx;
  border: 2rpx solid #666;
  height: 88rpx;
}
.user-time {
  margin: 0 !important;
  position: fixed;
  bottom: 194rpx;
  left: 50%;
  transform: translate(-50%, 0);
  width: 100%;
  padding: 0 50rpx;
}
.user-step {
  border-color: #fff !important;
}
.user-step .flex1 {
  border: 2rpx solid #ff3757 !important;
  border-radius: 60rpx;
}
.need-letter {
  letter-spacing: 10rpx;
}
.mr60 {
  margin-right: 60rpx;
}
.back-step {
  z-index: 999;
  bottom: 90rpx;
  border-color: #fff !important;
  color: #999;
}
.back-step .flex1 {
  border: 2rpx solid #fff !important;
  border-radius: 60rpx;
}
.pay-success-btn {
  bottom: 256rpx !important;
}
.icon-daosanjiao {
  position: relative;
  margin: 0;
  margin-top: 10rpx;
}
.icon-daosanjiao:before {
  position: absolute;
  top: 50%;
  right: 30rpx;
  transform: translate(0, -50%);
  font-size: 24rpx;
  color: #d8d8d8;
}
.finish-img image {
  width: 200rpx;
  height: 200rpx;
  display: inline-block;
}
.cff3a5a {
  border-color: #ff3a5a;
}
.mt60 {
  margin-top: 60rpx;
}
.mt120 {
  margin-top: 120rpx;
}
.circle {
  position: fixed;
  bottom: 50rpx;
  left: 50%;
  transform: translate(-50%, 0);
  display: flex;
  align-items: center;
  justify-content: center;
}
.circle view {
  font-size: 170rpx;
  height: 26rpx;
  line-height: 26rpx;
}
.learn-time {
  padding: 60rpx;
  padding-bottom: 0;
}
.s-i-5 {
  height: 100%;
}
.s-i-5 .bgfff {
  height: 100%;
}
.s-i-5 .user-time {
  width: 80%;
}
.success {
  font-size: 48rpx;
  font-weight: 700;
  text-align: center;
  margin-top: 70rpx;
}
.goods-btn {
  position: fixed;
  width: 90%;
  bottom: 50rpx;
  left: 50%;
  transform: translate(-50%, 0);
  background-color: #ff3b5b;
  margin: 0 !important;
  box-shadow: 0rpx 4rpx 10rpx 0rpx rgba(255, 228, 232, 0.7) !important;
}
.pay-img {
  margin-top: 160rpx;
}
.pay-img image {
  width: 446rpx;
  display: inline-block;
}
.apply-finish {
  padding-top: 160rpx;
}
.apply-finish image {
  width: 446rpx;
  display: inline-block;
}
.apply-last-img {
  width: 384rpx !important;
  height: 268rpx !important;
  margin-top: 86rpx !important;
}
.user-all-finish {
  padding: 0 !important;
  width: 90% !important;
}
.order-info {
  background-color: #f8f8f8;
}
.c-m-in {
  position: absolute;
  bottom: 0;
  left: 0;
  width: 100%;
  background-color: #fff;
  padding-top: 0;
}
.c-m-in .mask-in-top {
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.c-m-in .mask-in-top .p20 {
  padding: 20rpx 30rpx;
}
