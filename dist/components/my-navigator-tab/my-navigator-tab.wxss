.tab-wrap {
  display: flex;
  align-items: center;
  justify-content: space-around;
  padding: 10rpx 0;
  box-shadow: inset 0rpx 1rpx 0rpx 0rpx rgba(0, 0, 0, 0.1);
}
.tab-wrap .tab-item {
  flex: 1;
  display: flex;
  flex-direction: column;
  align-items: center;
  height: 130rpx;
  justify-content: flex-end;
}
.tab-wrap .tab-item image {
  max-width: 85rpx;
}
.tab-wrap .tab-item .f24 {
  margin-top: 24rpx;
}
.tab-wrap2 .tab-item {
  height: 92rpx;
}
.tab-wrap2 image {
  max-width: 50rpx !important;
}
.tab-wrap2 .f24 {
  margin-top: 10rpx !important;
}
.tab-wrap2 .active {
  color: #FF617B !important;
}
.bottom-operation {
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  background-color: #fff;
}
.f24 {
  font-size: 24rpx;
}
.c333 {
  color: #333;
}
.special {
  height: 9%;
}
.special .tab-item {
  height: 100% !important;
}
.form-button {
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
}
