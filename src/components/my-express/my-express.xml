<view class="mask {{ showS ? 'big' : 'small' }}" catchtap="_showScroll" data-type="check">
    <scroll-view catchtap="noUp" class="mask-scroll" scroll-y style="height: 80%; width: 80%; border-radius: 8rpx; background: #fff; padding: 20rpx;">
        <view class="f30 c333 fw7 tac">物流信息</view>
        <view wx:for="{{ express }}" wx:key="*94" style="font-size: 28rpx; color: #444; margin-top: 20rpx;">
            <view>{{ item.time }}</view>
            <view>{{ item.context }}</view>
        </view>
    </scroll-view>
    <view class="icon-cuowu iconfont cfff f40"></view>
</view>