               hover-class="none"
               class="tab-item tac f24 c333">
        <image lazy-load="{{true}}" mode="aspectFit" src="{{ item.icon }}"></image>
        <view class="f32 c3a">{{ item.title }}</view>
    </navigator>
</view>

