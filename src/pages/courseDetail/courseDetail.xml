<!--顶部视频区域-->
<view style="height: {{ needSmall ? 7 : 30 }}%;" class="video-area">
  <view wx:if="{{ !play }}" class="video-poster-img pr">
    <image class="video-poster"
           mode="aspectFill"
           src="{{ poster }}"></image>
    <view catchtap="videoPlay" class="video-btn iconfont icon-jinru pa"></view>
    <!--<view class="video-text pa fw7">可试看一分钟教程</view>-->
    <!--<view class="ask-question tac bgfff br10 p20">-->
      <!--<view class="f32 fw7">限时答题领取綉币</view>-->
