        <!--<view catchtap="startAnswer" data-type="open">参与答题</view>-->
        <!--<view class="cfff">继续下一集</view>-->
      <!--</view>-->
    <!--</view>-->
  </view>
  <video wx:else
         poster="{{ poster }}"
         controls="{{ controls }}"
         bindended="videoEnd"
         objectFit="fill"
         autoplay="{{ play }}"
         class="video-show"
         src="{{ videoSrc }}">
  </video>
</view>
<!--导航栏-->
<view class="tab-top bgfff lh88 bd1">
  <view wx:for="{{ videoTab }}"
        wx:key="*7"
        data-text="推荐{{index}}"
        catchtap="chooseIndex"
        data-id="s{{ index }}"
        data-index="{{ index }}"
        class="t-t-item f26 tac pr {{ currentIndex == index ? 'cff3b5b' : 'c666' }}">
    {{ item.t }}
  </view>
</view>
