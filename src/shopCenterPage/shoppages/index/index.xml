<form>
<view class="p20">
  <view class="br8 bgfff p300 f24 c444 goods-top">
    <navigator hover-class="none"
               wx:for="123"
               wx:key="*23"
               url="/shopGoodsPage/shoppages/index/index?type={{ index }}">
      <view>{{ index }}</view>
      <view>{{ index <= 0 ? '出售中的商品' : index == 1 ? '仓库中的商品' : '库存紧张的商品' }}</view>
    </navigator>
  </view>
  <view class="f28 c444 fw7 release mt20">
    <view class="r-item">发布新的商品</view>
    <view class="r-item">编辑运费模板</view>
  </view>
  <swiper class="mt20 br8 godds-swiper"
          circular="1"
          autoplay="1">
    <swiper-item wx:for="1324"
                 wx:key="*59">
      <image mode="aspectFill"
             class="br8"
             src="{{ img }}"></image>
    </swiper-item>
  </swiper>
  <view class="f28 c444 fw7 p30 br8 bgfff mt20">商品中心</view>
  <scroll-view scroll-x="{{true}}"
               class="goods-wrap bgfff">
    <view class="new-wrap-container">
      <button wx:for="{{ goodslabel }}"
              wx:key="*this"
              form-type="submit"
              class="f26 c666 goods-label flex1"
              hover-class="none">
        {{ item }}
      </button>
    </view>
  </scroll-view>
  <view url="/shopListPage/shoplistpages/detail/detail?id={{ item.id }}"
             hover-class="none"
             class="goods-list bt1 bgfff p30"
             wx:key="*011"
             wx:for="123123">
    <image mode="aspectFill"
           class="br8"
           src="{{ img }}"></image>
    <view class="flex1">
      <view class="f30 c444">我是商品标题，我总共有三行，我是商品标题</view>
      <view class="goods-bottom">
        <view class="f38 c444 fw7">¥88.00</view>
        <view class="f28 c444 goods-operation">
          <text data-id="{{ item.id }}" data-type="warehouse" catchtap="upGoods" class="mr20">放入仓库</text>
          <text data-id="{{ item.id }}" data-type="now" catchtap="upGoods">一键上传</text>
        </view>
      </view>
    </view>
  </view>
</view>
  <view class="not-agents">
    <button open-type="contact">联系客服开通店铺权限</button>
  </view>
</form>
<view class="zw"></view>
<my-navigator-tab></my-navigator-tab>
<!--错误弹窗-->
<import src="/template/template"></import>
<template wx:if="{{ toast.show }}" is="showToast" data="{{ ...toast }}"></template>
