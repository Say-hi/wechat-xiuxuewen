  "preloadRule": {
    "pages/index/index": {
      "network": "all",
      "packages": [
        "answer",
        "offline",
        "course",
        "new",
        "entering",
        "search",
        "shop"
      ]
    },
    "answerPage/pagesthree/answer/answer": {
      "network": "all",
      "packages": [
        "release"
      ]
    },
    "pages/user/user": {
      "network": "all",
      "packages": [
        "order",
        "collect",
        "store"
      ]
    },
    "storePage/pagesstore/index/index": {
      "network": "all",
      "packages": [
        "share"
      ]
    },
    "shopPage/shoppages/index/index": {
      "network": "all",
      "packages": [
        "car",
        "list",
        "video",
        "center",
        "user",
        "pictureShare"
      ]
    },
    "shopUserPage/shoppages/user/user": {
      "network": "all",
      "packages": [
        "shopOrder",
        "client",
        "money",
        "note",
        "release"
      ]
    },
    "shopCenterPage/shoppages/index/index": {
      "network": "all",
      "packages": [
        "edit",
        "release"
      ]
    },
    "shopMoneyPage/shoppages/index/index": {
      "network": "all",
      "packages": [
        "record",
        "withdraw"
      ]
    }
